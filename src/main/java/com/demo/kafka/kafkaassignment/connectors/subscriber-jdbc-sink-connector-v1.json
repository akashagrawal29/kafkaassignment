{
  "name": "subscriber-jdbc-sink-connector-v1",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
    "tasks.max": "1",
    "topics": "subscriber",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schema.registry.url": "http://locahost:8081",
    "value.converter.schema.registry.url": "http://schema-registry:8081",
    "connection.url": "jdbc:mysql://localhost:3306/kafkaDemo",
    "connection.user": "root",
    "connection.password": "Akash@12345",
    "insert.mode": "upsert",
    "batch.size": "2",
    "table.name.format": "kafkaDemo.subscriber",
    "pk.mode": "record_value",
    "pk.fields": "case_number",
    "db.timezone": "Asia/Kolkata"
  }
}